                                 />

        <addForeignKeyConstraint baseColumnNames="manager_person_id"
                                 baseTableName="tb_person"
                                 constraintName="fk_tb_person__manager_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_person"
                                 />
    </changeSet>
</databaseChangeLog>
